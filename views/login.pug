extends layout

block content

  h1= title
  p Welcome to #{title} page

  form(action="/login", method="post")
    div
        label(for='uname') Username:
        input#uname(type="text",name="username")
    div
        label(for='pword') Password:
        input#pword(type="password",name="password")
    div
        input(type="submit", value="Log In")

